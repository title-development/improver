{
  "name": "improver",
  "version": "3.0.0-SNAPSHOT",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf ./target/classes",
    "pretest": "webdriver-manager update",
    "start": "ng serve --prod",
    "serve": "ng serve --hmr",
    "serve:test": "ng serve --configuration=test",
    "build": "ng build",
    "build:test": "ng build --configuration=test",
    "build:qa": "node --max_old_space_size=6096 node_modules/@angular/cli/bin/ng build --configuration=qa --source-map",
    "build:prod": "node --max_old_space_size=6096 node_modules/@angular/cli/bin/ng build --prod && npm run allow-crawling",
    "build:stats": "ng build --stats-json",
    "allow-crawling": "rimraf ./target/classes/robots.txt",
    "bundle-report": "webpack-bundle-analyzer target/classes/stats.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e --port=4202",
    "webdriver-update": "webdriver-manager update"
  },
